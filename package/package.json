{
    "name": "@scrolia/react",
    "version": "0.6.0",
    "description": "A headless scrollbar component",
    "keywords": [
        "scrolia",
        "scrollbar",
        "react",
        "headless",
        "component",
        "ts",
        "typescript",
        "js",
        "javascript"
    ],
    "homepage": "https://github.com/scrolia/react",
    "bugs": "https://github.com/scrolia/react/issues",
    "repository": {
        "type": "git",
        "url": "https://github.com/scrolia/react.git",
        "directory": "package"
    },
    "license": "MIT",
    "author": {
        "name": "Alpheus Tang",
        "url": "https://alpheus.day"
    },
    "exports": {
        ".": {
            "types": "./dist/index.d.ts",
            "import": "./dist/index.mjs",
            "require": "./dist/index.js"
        },
        "./contexts/scrollcore": {
            "types": "./dist/contexts/scrollcore.d.ts",
            "import": "./dist/contexts/scrollcore.mjs",
            "require": "./dist/contexts/scrollcore.js"
        },
        "./functions/props": {
            "types": "./dist/functions/props.d.ts",
            "import": "./dist/functions/props.mjs",
            "require": "./dist/functions/props.js"
        },
        "./hooks/length": {
            "types": "./dist/hooks/length.d.ts",
            "import": "./dist/hooks/length.mjs",
            "require": "./dist/hooks/length.js"
        },
        "./hooks/scroll": {
            "types": "./dist/hooks/scroll.d.ts",
            "import": "./dist/hooks/scroll.mjs",
            "require": "./dist/hooks/scroll.js"
        },
        "./hooks/thumb/x": {
            "types": "./dist/hooks/thumb/x.d.ts",
            "import": "./dist/hooks/thumb/x.mjs",
            "require": "./dist/hooks/thumb/x.js"
        },
        "./hooks/thumb/y": {
            "types": "./dist/hooks/thumb/y.d.ts",
            "import": "./dist/hooks/thumb/y.mjs",
            "require": "./dist/hooks/thumb/y.js"
        },
        "./package.json": "./package.json"
    },
    "main": "./dist/index.js",
    "module": "./dist/index.mjs",
    "types": "./dist/index.d.ts",
    "files": [
        "dist"
    ],
    "dependencies": {
        "ts-vista": "~0.2.2"
    },
    "devDependencies": {
        "@types/react": "~19",
        "react": "19.0.0"
    },
    "peerDependencies": {
        "react": "^19.0.0"
    }
}
